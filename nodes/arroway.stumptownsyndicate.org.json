{
  "run_list": [
    "role[nginx]",
    "role[proxied-apache]",
    "role[mysql]",
    "role[osbridge-ocw]",
    "role[osbridge-volunteer]",
    "role[wordpress]",
    "role[mediawiki]",
    "recipe[memcached]",
    "recipe[php-apache2-upload-size]",
    "recipe[osbridge]"
  ],
  "users": [
    "afarrell"
  ],
  "set_fqdn": "arroway.stumptownsyndicate.org",
  "server-banner": {
    "link": "http://stumptownsyndicate.org/wiki/Tech/Server_Notes#arroway.stumptownsyndicate.org",
    "image": "arroway.jpg",
    "background_color": "#070A09",
    "text_color": "#D8E1E0"
  },
  "syndicate-wordpress": {
    "multisite": false,
    "title": "Open Source Bridge",
    "url": "http://opensourcebridge.org",
    "admin_email": "web@opensourcebridge.org",
    "db": {
      "name": "osbridge_wordpress"
    },
    "plugins": [
      "markdown-on-save",
      "browserid",
      "advanced-category-excluder",
      "jetpack",
      "akismet",
      "numeric-slugs",
      "backtype-connect",
      "embed-iframe",
      "feedburner-plugin",
      "google-analyticator",
      "google-sitemap-generator",
      "markdown-on-save",
      "podpress",
      "raw-html",
      "redirection",
      "stats",
      "text-control",
      "w3-total-cache",
      "wphone",
      "wp-syntax"
    ],
    "sites": [
      {
        "server_name": "opensourcebridge.org",
        "server_aliases": [
          "www.opensourcebridge.org",
          "osbridge.local"
        ],
        "theme": {
          "type": "git",
          "name": "osbp_wordpress_theme_v3",
          "repo": "https://github.com/osbridge/osbp_wordpress.git"
        },
        "port": 5500,
        "ssl_enabled": false,
        "aliases": {
          "/wiki": "/var/www/mediawiki/index.php",
          "/wiki-raw": "/var/www/mediawiki/"
        }
      }
    ]
  }
}
